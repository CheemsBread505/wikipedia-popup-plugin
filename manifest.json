{
    "manifest_version": 2,
  
    "name": "Wikipedia popup", 
    "description": "A plugin that makes a popup with wikipedia embeded in it.",
    "version": "0.1",
    "author": "Cheems Brewad",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html",
        "default_title": "Wikipedia popup",
        "icons": {
            "16": "img/icon.png",
            "48": "img/icon.png",
           "128": "img/icon.png"
         }       
        
    },
    "permissions": [
        "activeTab"
     ]
  }